{
  "name": "selectivity",
  "desc": "Modular and light-weight selection library for jQuery and Zepto.js",
  "version": "3.0.0-alpha",
  "repository": {
    "type": "git",
    "url": "git://github.com/arendjr/selectivity.git"
  },
  "scripts": {
    "build-jquery-min": "gulp --api=jquery --bundle-name=jquery --exclude-modules=plugins/options-validator --minify",
    "build-jquery-source": "gulp --api=jquery --bundle-name=jquery --derequire",
    "build": "npm run build-jquery-source && npm run build-jquery-min",
    "test": "gulp eslint && gulp unit-tests"
  },
  "devDependencies": {
    "browser-sync": "^2.14.0",
    "browserify": "^13.0.0",
    "bundle-collapser": "^1.1.0",
    "eslint": "^3.0.0",
    "faucet": "0.0.1",
    "freshy": "^1.0.2",
    "glob": "^7.0.3",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.4.3",
    "gulp-csso": "^2.0.0",
    "gulp-derequire": "^2.0.0",
    "gulp-eslint": "^3.0.0",
    "gulp-header": "^1.7.1",
    "gulp-if": "^2.0.0",
    "gulp-nodeunit": "0.0.5",
    "gulp-replace": "^0.5.0",
    "gulp-ruby-sass": "~2.0.6",
    "gulp-tape": "0.0.9",
    "gulp-uglify": "^1.0.2",
    "gulp-util": "^3.0.1",
    "jsdom": "^9.1.0",
    "tape": "^4.5.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "yargs": "^4.7.1"
  },
  "keywords": [
    "dropdown",
    "ecosystem:jquery",
    "ecosystem:react",
    "form",
    "input",
    "jquery-plugin",
    "multiselect",
    "select",
    "selectivity",
    "submenu",
    "tag",
    "tagging",
    "ui"
  ],
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.0.0",
    "lodash": "^4.13.1"
  }
}
